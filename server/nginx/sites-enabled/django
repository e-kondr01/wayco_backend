server {
    listen 80;
    server_name 188.227.86.62;

    location / {
        # django running in uWSGI
        uwsgi_pass unix:///run/uwsgi/app/django/socket;
        include uwsgi_params;
        uwsgi_read_timeout 300s;
        client_max_body_size 32m;
    }

    location /static/ {
       # static files
       alias /wayco/static/; # ending slash is required
    }

    location /media/ {
        # media files, uploaded by users
        alias /wayco/media/; # ending slash is required
    }
}